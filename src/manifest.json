{
  "manifest_version": 3,
  "name": "BoardGameArena Extension",
  "description": "A chrome extension for https://boardgamearena.com/",
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://boardgamearena.com/1/*",
        "https://boardgamearena.com/2/*",
        "https://boardgamearena.com/3/*",
        "https://boardgamearena.com/4/*",
        "https://boardgamearena.com/5/*",
        "https://boardgamearena.com/6/*",
        "https://boardgamearena.com/7/*",
        "https://boardgamearena.com/8/*",
        "https://boardgamearena.com/9/*",
        "https://boardgamearena.com/10/*",
        "https://boardgamearena.com/11/*",
        "https://boardgamearena.com/12/*",
        "https://boardgamearena.com/13/*",
        "https://boardgamearena.com/14/*",
        "https://boardgamearena.com/15/*",
        "https://boardgamearena.com/16/*",
        "https://boardgamearena.com/17/*",
        "https://boardgamearena.com/18/*",
        "https://boardgamearena.com/19/*",
        "https://boardgamearena.com/20/*"
      ],
      "run_at": "document_start",
      "js": [
        "contentScript.bundle.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": [
        "https://boardgamearena.com/*"
      ]
    }
  ]
}